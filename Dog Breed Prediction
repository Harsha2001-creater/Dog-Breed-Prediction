      "cell_type": "code",
      "source": [
        "# Function to load sampled images from a directory\n",
        "def load_sampled_images(folder_path, sample_ratio=0.1):\n",
        "    images = []\n",
        "    file_names = os.listdir(folder_path)\n",
        "    sampled_files = random.sample(file_names, int(len(file_names) * sample_ratio))\n",
        "    for file_name in sampled_files:\n",
        "        if file_name.endswith('.jpg') or file_name.endswith('.jpeg') or file_name.endswith('.png'):\n",
        "            image_path = os.path.join(folder_path, file_name)\n",
        "            img = Image.open(image_path)\n",
        "            images.append((img, folder_path.split('/')[-1]))  # Assign label based on folder name\n",
        "    return images"
      ],
      "metadata": {
        "id": "txrVwvWDyWMa"
      },
      "execution_count": null,
      "outputs": []
